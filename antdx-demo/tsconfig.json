{
    "compilerOptions": {
      "outDir": "./dist",
      "rootDir": "./src",
      "target": "es6",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "allowJs": true,
      "checkJs": false,
      "esModuleInterop": true,
      "strict": false,
      "noImplicitAny": false,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }